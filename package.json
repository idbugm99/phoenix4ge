{
  "name": "phoenix4ge",
  "version": "1.0.0",
  "description": "Phoenix Forge - Professional model portfolio management system built with Node.js",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "migrate": "node scripts/migrate.js",
    "migrate:extra": "node scripts/migrate-extra.js",
    "migrate-data": "node scripts/migrate-data.js",
    "seed": "node scripts/seed.js",
    "verify-db": "node scripts/verify-db.js",
    "route-audit": "node scripts/route-audit.js",
    "verify-migrations": "node scripts/verify-migrations.js",
    "verify-api-mounts": "node scripts/verify-api-mounts.js",
    "smoke:api": "node scripts/smoke-api.js",
    "check": "npm run verify-db && npm run route-audit && npm run verify-migrations && npm run verify-api-mounts && npm run smoke:api",
    "ai:smoke": "node tests/api/test-ai-integration.js",
    "backfill:gallery": "node scripts/backfill_gallery.js $MODEL_SLUG",
    "uploads:normalize": "node scripts/normalize_uploads.js $MODEL_SLUG",
    "build:sprites": "node scripts/build-royal-sprite.js",
    "build": "npm run build:sprites",
    "theme:register": "node scripts/register-royal-gem-theme.js"
  },
  "keywords": [
    "model",
    "portfolio",
    "cms",
    "escort",
    "booking"
  ],
  "author": "Phoenix Forge",
  "license": "MIT",
  "dependencies": {
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.11.0",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-handlebars": "^8.0.3",
    "express-rate-limit": "^6.8.1",
    "express-session": "^1.17.3",
    "express-validator": "^7.0.1",
    "file-type": "^18.7.0",
    "form-data": "^4.0.4",
    "handlebars": "^4.7.8",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.0",
    "node-appwrite": "^20.3.0",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.11",
    "passport": "^0.7.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "qrcode": "^1.5.4",
    "redis": "^5.8.1",
    "sharp": "^0.34.3",
    "speakeasy": "^2.0.0",
    "sqlite3": "^5.1.7",
    "uuid": "^9.0.1",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "jest": "^29.6.2",
    "nodemon": "^3.1.10"
  },
  "engines": {
    "node": ">=16.0.0"
  }
